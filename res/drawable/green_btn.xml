<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ff1ec51c" />
            <gradient  android:angle="-90"  android:startColor="#ff075f21" android:endColor="#ff3ed600"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ff1c9926" />
            <solid  android:color="#ff1c9926"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ff075f21" />
            <gradient  android:angle="-90"  android:startColor="#ff1ec51c" android:endColor="#ff1c9926" />
        </shape>
    </item>
</selector>